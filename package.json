{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cp .env.production .env && NODE_ENV=prd nuxt build",
    "build:stg": "cp .env.dev .env && NODE_ENV=dev nuxt build",
    "dev": "cp .env.production .env && NODE_ENV=prd nuxt dev --port=3001",
    "dev:stg": "cp .env.dev .env && NODE_ENV=dev nuxt dev --port=3001",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "cp .env.production .env && NODE_ENV=prd PORT=3001 nuxt start",
    "start:stg": "cp .env.dev .env && NODE_ENV=dev PORT=3001 nuxt start"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@types/js-cookie": "^3.0.6",
    "autoprefixer": "^10.4.16",
    "dayjs-nuxt": "^2.1.9",
    "nuxt": "^3.8.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@iconify/vue": "^4.1.1",
    "@mdi/font": "^7.3.67",
    "@nuxtjs/dayjs": "^1.4.1",
    "@pinia/nuxt": "^0.5.1",
    "@types/node": "^20.10.0",
    "axios": "^1.6.0",
    "firebase": "^10.5.2",
    "humps": "^2.0.1",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "pinia": "^2.1.7",
    "primevue": "^3.39.0",
    "sass": "^1.69.5",
    "sweetalert2": "^11.9.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vuetify": "^3.4.0-alpha.1"
  }
}